{
  "settings": {
    // Outgoing traffic
    "OutgoingImporter": {
      "class": "TelemetryImporter",
      "ip": "localhost",
      "nic": "localhost",
      "port": "9092",
      "clientId": "*",
      "clientType": "*",
      "typeTopicMap": {
        "GcsLightsRep": "telemetry_lights",
        "GcsFastInsRpt": "telemetry_fast_ins"
      }
    },
    "OutgoingProcessors": [ "Telemetry2JsonProcessor" ],
    "OutgoingExporter": {
      "class": "KafkaExporter",
      "ip": "localhost",
      "nic": "localhost",
      "port": "9092",
      "clientId": "Demo",
      "clientType": "GCS",
      "typeTopicMap": {
        "GcsLightsRep": "telemetry_lights",
        "GcsFastInsRpt": "telemetry_fast_ins"
      }
    },

    // Incoming traffic
    "IncomingImporter": {
      "class": "KafkaImporter",
      "ip": "localhost",
      "nic": "localhost",
      "port": "9092",
      "clientId": "*",
      "clientType": "IncomingTelemetry",
      "typeTopicMap": {
        "GcsLightsRep": "telemetry_lights"
      }
    },
    "IncomingProcessors": [ "Json2TelemetryProcessor" ],
    "IncomingExporter": {
      "class": "ConsoleExporter",
      "ip": "localhost",
      "nic": "localhost",
      "port": "9092",
      "clientId": "Demo",
      "clientType": "GCS",
      "typeTopicMap": {
      }
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
